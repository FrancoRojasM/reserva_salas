@using Microsoft.AspNetCore.Http
@{
    ViewBag.Title = "ListaCadenaPoa";
    Layout = "~/Views/Shared/Inicial.cshtml";
    Context.Session.SetString("ModuloActual", "Reurso Humano");
    Context.Session.SetString("PaginaActual", "Reportes");
    Context.Session.SetString("SubPaginaActual", "Empleado");
    @if (ViewBag.conpermiso == 0)
    {
<div style="font-weight:bold;color:red;">Usted no tiene permiso para ver este recurso</div>
    }
    else
    {
<iframe style="width:100%; height:760px;"
        src="https://PROVIASreporte.netwaresoft.com/ReportViewer/Home/ReporteByEmpleado?IdUser=@ViewBag.IdUsuario&clave=@ViewBag.Clave" 
        frameborder="0" allowfullscreen>
</iframe>
        }
    }
